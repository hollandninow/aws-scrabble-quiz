{
  "name": "scrabble-quiz",
  "version": "1.0.0",
  "description": "Flash card quiz app to train Scrabble words",
  "engines": {
    "node": ">=8.3.0"
  },
  "main": "app.js",
  "scripts": {
    "start": "node server.js",
    "start:dev": "SET NODE_ENV=development& nodemon server.js",
    "start:prod": "SET NODE_ENV=production& nodemon server.js",
    "debug": "ndb server.js",
    "debug:prod": "SET NODE_ENV=production& ndb server.js",
    "watch:js": "parcel serve ./public/js/index.js --dist-dir ./public/js/bundled",
    "build:js": "parcel build ./public/js/index.js --dist-dir ./public/js/bundled"
  },
  "author": "Holland Ninow",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^6.7.0",
    "helmet": "^6.0.1",
    "hpp": "^0.2.3",
    "html-to-text": "^9.0.3",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^6.8.1",
    "morgan": "^1.10.0",
    "nodemailer": "^6.8.0",
    "pug": "^3.0.2",
    "validator": "^13.7.0",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "eslint": "^8.30.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "parcel": "^2.8.2",
    "prettier": "^2.8.1"
  }
}
